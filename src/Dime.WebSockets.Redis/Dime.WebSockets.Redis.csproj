<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net8.0</TargetFrameworks>
		<Authors>Dime Software</Authors>
		<Company>Dime Software</Company>
		<Product>Dime.WebSockets</Product>
		<Copyright>Copyright Â© 2023</Copyright>
		<PackageIconUrl>https://cdn.dime-software.com/dime-software/logo-shape.png</PackageIconUrl>
		<PackageTags>Websockets;SignalR</PackageTags>
		<RepositoryUrl>https://github.com/dimesoftware/websockets</RepositoryUrl>
		<PackageProjectUrl>http://www.dimescheduler.com</PackageProjectUrl>
		<RepositoryUrl>https://github.com/dimesoftware/websockets</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<NeutralLanguage>en</NeutralLanguage>
		<Description>Web socket connection tracker using Redis</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<IncludeSource>True</IncludeSource>
		<IncludeSymbols>True</IncludeSymbols>
		<Version>2.1.0</Version>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<LangVersion>latest</LangVersion>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Dime.WebSockets\Dime.WebSockets.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="ServiceStack.Redis.Core" Version="8.4.0" />
	</ItemGroup>

</Project>
